{
  "private": true,
  "name": "blog",
  "version": "1.0.26",
  "scripts": {
    "build": "lerna run build --parallel",
    "build-studio": "lerna bootstrap && (cd studio && yarn run build)",
    "build-web": "lerna bootstrap && (cd web && yarn run build)",
